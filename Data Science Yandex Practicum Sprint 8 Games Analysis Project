   "source": [
    "# сформируем актуальный датасет для дальнейшего исследования \n",
    "actual_game_data = game_data[game_data['year_of_release'] > 2013]\n",
    "# и запишем список актуальных топ-6 платформ \n",
    "actual_top_six = ['PS4', 'XOne', '3DS', 'WiiU', 'PC', 'PSV']"
   ]
