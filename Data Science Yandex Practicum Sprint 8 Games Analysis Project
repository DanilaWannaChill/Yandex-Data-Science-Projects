    "#Откроем файл с данными безопасным методом try/except\n",
    "try:\n",
    "    game_data = pd.reead_csv('/datasets/games.csv')\n",
    "except:\n",
    "    game_data = pd.read_csv('https://code.s3.yandex.net/datasets/games.csv')"
   ]
